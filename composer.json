{
    "name": "linkorb/librealm",
    "description": "A library providing a Model of Realm and Peri22 data",
    "license": "MIT",
    "require": {
        "php": "^5.5.9 || >= 7.0.8",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/serializer": "^3.4",
        "symfony/property-access": "^3.4",
        "symfony/property-info": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "LinkORB\\Realm\\": "lib/Realm"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
